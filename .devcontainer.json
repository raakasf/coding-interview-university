{
	"image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:0-12",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint"
			]
		}
	},
	"forwardPorts": [
		3000
	],
	"features": {
		"common": {
			"username": "automatic",
			"uid": "automatic",
			"gid": "automatic",
			"installZsh": true,
			"installOhMyZsh": true,
			"upgradePackages": true,
			"nonFreePackages": false
		},
		"docker-in-docker": {
			"version": "latest",
			"moby": true,
			"dockerDashComposeVersion": "v1"
		},
		"github-cli": "latest",
		"golang": "latest",
		"homebrew": {
			"version": "latest",
			"shallow": false
		},
		"kubectl-helm-minikube": {
			"version": "latest",
			"helm": "latest",
			"minikube": "latest"
		},
		"python": "latest",
		"rust": {
			"version": "latest",
			"profile": "minimal"
		}
	}
}
